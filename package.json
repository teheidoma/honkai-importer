{
  "name": "honkai-importer",
  "version": "0.0.1",
  "author": {
    "url": "https://teheidoma.com"
  },
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --configuration development",
    "start": "electron-forge start",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "pack": "ng build --base-href=./ && electron-builder --dir",
    "dist": "ng build --base-href=./ && electron-builder"
  },
  "repository": {
    "url": "https://github.com/teheidoma/honkai-importer.git",
    "type": "git"
  },
  "build": {
    "appId": "com.teheidoma.honkai-importer",
    "win": {
      "target": "nsis"
    },
    "icon": null
  },
  "private": true,
  "main": "main.js",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "anychart": "^8.11.0",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "lodash": "^4.17.21",
    "regedit": "^5.1.2",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.7.5",
    "tslib": "^2.3.0",
    "update-electron-app": "^2.0.1",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular/cli": "~16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.14.194",
    "autoprefixer": "^10.4.14",
    "electron": "24.3.0",
    "electron-builder": "^23.6.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.2"
  }
}
